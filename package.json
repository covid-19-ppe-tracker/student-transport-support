{
  "name": "student-transport-support",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run migrate && npm run seed && node ./bin/www",
    "start:dev": "nodemon ./bin/www | pino-pretty --translateTime='HH:MM:ss'",
    "generateKeys": "node generate-vapid-keys.js",
    "db:drop": "npx sequelize-cli db:drop",
    "db:create": "npx sequelize-cli db:create",
    "db:rebuild": "npm run db:drop && npm run db:create && npm run migrate",
    "migrate": "npx sequelize db:migrate",
    "migrate:undo": "npx sequelize db:migrate:undo",
    "migration:create": "npx sequelize migration:create --name",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo --seed",
    "bundle": "NODE_ENV=production node bin/bundle",
    "deploy": "npm run bundle && gcloud app deploy && rm .adminbro/bundle.js"
  },
  "dependencies": {
    "@google-cloud/storage": "^4.7.0",
    "admin-bro": "^2.2.3",
    "admin-bro-expressjs": "^2.0.1",
    "admin-bro-sequelizejs": "^0.4.0",
    "argon2": "^0.26.1",
    "cookie-parser": "^1.4.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "~4.16.1",
    "express-formidable": "^1.2.0",
    "express-pino-logger": "^4.0.0",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "faker": "^5.1.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-google-storage": "^1.3.0",
    "nanoid": "^3.0.2",
    "nodemailer": "^6.4.6",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "sqlite3": "^4.1.1",
    "web-push": "^3.4.3"
  },
  "engines": {
    "node": "12.x.x"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "pino-pretty": "^4.0.0"
  }
}
